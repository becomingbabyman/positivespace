<div id='users'>
	<br/>
	<br/>
	<br/>
	<div class="container" ng-hid='app.loggedIn()'>
		<div class="row pbm">
			<div class="span12 center">
				<h1 class='font48'>Know more people</h1>
			</div>
		</div>
		<div class="row pbl">
			<div class="span12">
				<div class='animated-header'>
